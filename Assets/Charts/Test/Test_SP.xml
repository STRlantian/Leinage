<?xml version="1.0" encoding="utf-8" ?>
<chart>
	<!--name:名字 ; level:难度名 ; diff:定数 ; rhythm:拍号(拍数:时值) ; bpm:每分钟节拍 ; time:秒时间 ; offset:延时-->
	<info name="Test" level="SP" diff="12" rhythm="4:4" bpm="140" speed="1" time="122" offset="20" composer="STRlantian" author="STRlantian" illust="NULL"></info>
	
	<blocks>
		<!--type: 什么几何体 tetra, hex, octa, dode, ico-->
		<!--英语小常识: 以上分别对应 正四面体 正六面体 正八面体 正十二面体 正二十面体-->
		<!--目前来说 只能实现正六面体(然后是四面 等等)-->
		<!--结构采用嵌套的方式 应该可以看懂吧(?-->
		<block type="hex" back="false">
			sample
			<pane>
				sample_pane1
				<line>
					sample_pane1_line1
					<notes>
						<!--type:种类(tap0 flick1 drag2 hold3) ; beat:小节数:拍数:几分音符; x:相对x位置 ; multi:多押特效-->
						<!--block:方块名字 ; pane:面 ; -->
						<note type="tap" beat="1:1:4" block="sample" pane="A" line="down" x="5" speed="1" out="false" multi="false"></note>
						<note type="flick" beat="1:2:4" block="sample" pane="A" line="down" x="5" speed="1" out="false" multi="false"></note>
						<note type="drag" beat="1:3:4" block="sample" pane="A" line="down" x="5" speed="1" out="false" multi="false"></note>
						<!--对于hold的beat: 开始小节数:拍数:几分音符:结束小节数:结束拍:几分音符-->
						<note type="hold" beat="1:4:4:2:4:4" block="sample" pane="A" line="down" x="5" speed="1" out="true" multi="false"></note>
					</notes>
				</line>

				<line>
					sample_pane1_line2

				</line>

				<line>
					sample_pane1_line3

				</line>

				<line>
					sample_pane1_line4

				</line>
			</pane>
			
		</block>
		
	</blocks>

	<events>
	</events>
</chart>