<?xml version="1.0" encoding="utf-8" ?>
<chart>
	<!--name:名字 ; level:难度名 ; diff:定数 ; rhythm:拍号 ; bpm:每分钟节拍 ; time:秒时间 ; offset:延时-->
	<info name="Test" level="SP" diff="12" rhythm="4:4" bpm="140" time="233" offset="20"></info>
	
	<notes>
		<!--type: 种类 ; beat: 小节数:拍数:几分音符; x: 相对x位置 ; multi: 多押特效--> 
		<note type="tap" beat="1:1:4" pane="A" toline="down" x="5" speed="1" out="false" multi="false"></note>
		<note type="flick" beat="1:2:4" pane="A" toline="down" x="5" speed="1" out="false" multi="false"></note>
		<note type="drag" beat="1:3:4" pane="A" toline="down" x="5" speed="1" out="false" multi="false"></note>
		<!--对于hold和spin的beat: 开始小节数:拍数:结束小节数:结束拍:几分音符-->
		<note type="hold" beat="1:4:2:4:4" pane="A" toline="down" x="5" speed="1" out="true" multi="false"></note>
		<!--spin特殊属性: dest:三个冒号分割x:y:z的旋转坐标和已经有的圈数,如30x1意思是转到360+30度-->
		<!--infin:是否无限转-->
	    <note type="spin" infin="false" beat="2:4:4:4:4" dest="0x1:180x0:80x0" multi="false"></note>
	</notes>
	
	<events>
	</events>
</chart>
